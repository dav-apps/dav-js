{
	"name": "dav-js",
	"version": "2.0.10",
	"description": "JavaScript library for accessing the dav API",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"scripts": {
		"test": "npm run compile && mocha --require mock-local-storage dist/test/*.spec.js dist/test/**/*.spec.js",
		"prepublishOnly": "npm run build",
		"compile": "npm run clean && tsc -p . && npm run copyJs",
		"watch": "tsc -w -p .",
		"build": "npm run compile",
		"prepare": "npm run compile",
		"clean": "rm -rf dist",
		"copyJs": "cpx lib/workers/*.js dist/lib/workers"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/dav-apps/dav-js.git"
	},
	"author": "Dav",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/dav-apps/dav-js/issues"
	},
	"homepage": "https://github.com/dav-apps/dav-js",
	"files": [
		"dist/index.js",
		"dist/index.d.ts",
		"dist/lib"
	],
	"dependencies": {
		"axios": "^0.21.4",
		"crypto-js": "^4.1.1",
		"localforage": "^1.10.0",
		"localforage-startswith": "^1.4.0"
	},
	"devDependencies": {
		"@types/chai": "^4.2.22",
		"@types/crypto-js": "^4.0.1",
		"@types/mocha": "^9.0.0",
		"@types/moxios": "^0.4.12",
		"@types/node": "^14.14.22",
		"chai": "^4.3.4",
		"cpx": "^1.5.0",
		"mocha": "^9.1.2",
		"mock-local-storage": "^1.1.17",
		"moxios": "^0.4.0",
		"typescript": "^4.4.4"
	}
}
