{
	"name": "dav-js",
	"version": "2.7.1",
	"description": "JavaScript library for accessing the dav API",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"scripts": {
		"test": "npm run compile && mocha --require mock-local-storage dist/test/*.spec.js dist/test/controllers/*.spec.js dist/test/models/*.spec.js dist/test/providers/*.spec.js",
		"prepublishOnly": "npm run build",
		"compile": "npm run clean && tsc -p . && npm run copyJs",
		"watch": "tsc -w -p .",
		"build": "npm run compile",
		"prepare": "npm run compile",
		"clean": "rm -rf dist",
		"copyJs": "cpx lib/workers/*.js dist/lib/workers"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/dav-apps/dav-js.git"
	},
	"author": "Dav",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/dav-apps/dav-js/issues"
	},
	"homepage": "https://github.com/dav-apps/dav-js",
	"files": [
		"dist/index.js",
		"dist/index.d.ts",
		"dist/lib"
	],
	"dependencies": {
		"axios": "^1.6.8",
		"crypto-js": "^4.2.0",
		"localforage": "^1.10.0",
		"localforage-startswith": "^1.4.0"
	},
	"devDependencies": {
		"@types/chai": "^4.3.12",
		"@types/crypto-js": "^4.2.2",
		"@types/mocha": "^10.0.6",
		"@types/node": "^20.11.28",
		"axios-mock-adapter": "^1.22.0",
		"chai": "^5.1.0",
		"cpx": "^1.5.0",
		"mocha": "^10.3.0",
		"mock-local-storage": "^1.1.24",
		"typescript": "^5.4.2"
	}
}
