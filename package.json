{
	"name": "dav-js",
	"version": "2.6.6",
	"description": "JavaScript library for accessing the dav API",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"scripts": {
		"test": "npm run compile && mocha --require mock-local-storage dist/test/*.spec.js dist/test/controllers/*.spec.js dist/test/models/*.spec.js",
		"prepublishOnly": "npm run build",
		"compile": "npm run clean && tsc -p . && npm run copyJs",
		"watch": "tsc -w -p .",
		"build": "npm run compile",
		"prepare": "npm run compile",
		"clean": "rm -rf dist",
		"copyJs": "cpx lib/workers/*.js dist/lib/workers"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/dav-apps/dav-js.git"
	},
	"author": "Dav",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/dav-apps/dav-js/issues"
	},
	"homepage": "https://github.com/dav-apps/dav-js",
	"files": [
		"dist/index.js",
		"dist/index.d.ts",
		"dist/lib"
	],
	"dependencies": {
		"axios": "^1.3.4",
		"crypto-js": "^4.1.1",
		"localforage": "^1.10.0",
		"localforage-startswith": "^1.4.0"
	},
	"devDependencies": {
		"@types/chai": "^4.3.4",
		"@types/crypto-js": "^4.1.1",
		"@types/mocha": "^10.0.1",
		"@types/node": "^18.14.2",
		"axios-mock-adapter": "^1.21.4",
		"chai": "^4.3.7",
		"cpx": "^1.5.0",
		"mocha": "^10.2.0",
		"mock-local-storage": "^1.1.23",
		"typescript": "^4.9.5"
	}
}
